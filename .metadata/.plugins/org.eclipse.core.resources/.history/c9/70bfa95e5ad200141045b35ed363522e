package Main;

import java.util.ArrayList;

public class PrimeFactorization {
	
	public static int[] primeFactor(int number) {
		return number == 1? new int[0] : 
			(number == 2? new int[] {2} : 
				(number == 3? new int[] {3}: 
					(number == 4? new int[] {2,2} : 
						(number ==6 ? new int[] {2,3}: new int[] {2,2,2}))));
	}
}
